    ],
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/kiritokatklian"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "calebporzio/sushi": "^2.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Kurozora\\SortRequest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kurozora\\SortRequest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kurozora\\SortRequest\\SortRequestServiceProvider"
            ]
        }
    }
}
