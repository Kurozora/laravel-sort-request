{
    "name": "musa11971/laravel-sort-request",
    "description": "TBA",
    "keywords": [
        "laravel-sort-request"
    ],
    "homepage": "https://github.com/musa11971/laravel-sort-request",
    "license": "MIT",
    "authors": [
        {
            "name": "musa11971",
            "email": "mussesemou99@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "calebporzio/sushi": "^2.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "musa11971\\SortRequest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "musa11971\\SortRequest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "musa11971\\SortRequest\\SortRequestServiceProvider"
            ]
        }
    }
}
