{
    "name": "musa11971/laravel-sort-request",
    "description": "TBA",
    "keywords": [
        "laravel-sort-request"
    ],
    "homepage": "https://github.com/musa11971/laravel-sort-request",
    "license": "MIT",
    "authors": [
        {
            "name": "musa11971",
            "email": "mussesemou99@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "calebporzio/sushi": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.2",
        "spatie/phpunit-snapshot-assertions": "^3.1",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "musa11971\\SortRequest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "musa11971\\SortRequest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "musa11971\\SortRequest\\SortRequestServiceProvider"
            ]
        }
    }
}
